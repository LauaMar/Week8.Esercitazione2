@model ProductViewModel

<div class="card" style="width: 30rem;">
    <div class="card-body">
        <h4 class="card-title">@Model.ProductCode</h4>
        <h5 class="card-text">[@Model.Type] - @Model.Description  </h5>
        <p class="card-text">Bought at @Model.BuyingPrice € - Sold at @Model.SellingPrice €</p>
    </div>
</div>